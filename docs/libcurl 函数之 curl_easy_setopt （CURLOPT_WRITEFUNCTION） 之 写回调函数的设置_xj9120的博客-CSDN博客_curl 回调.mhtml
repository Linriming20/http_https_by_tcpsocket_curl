  curl_easy_setopt(curl_handle, CURLOPT_WRITEFUNCTION, WriteMemoryCallback)=
;
 =20
=20
  /* we pass our 'chunk' struct to the callback function */=20
  curl_easy_setopt(curl_handle, CURLOPT_WRITEDATA, (void *)&amp;chunk);
=20
  /* some servers don't like requests that are made without a user-agent   =
field, so we provide one */=20
  curl_easy_setopt(curl_handle, CURLOPT_USERAGENT, "libcurl-agent/1.0");
=20
  /* get it! */=20
  //=E5=AF=B9=E4=BA=8E=E5=90=8C=E4=B8=80=E6=AC=A1=E9=98=BB=E5=A1=9E=E7=9A=
