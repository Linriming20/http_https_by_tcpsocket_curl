=8C=BA=E7=9A=84=E5=A4=A7=E5=B0=8F
  char *ptr =3D realloc(mem-&gt;memory, mem-&gt;size + realsize + 1);=20
  if(ptr =3D=3D NULL) {
    /* out of memory! */=20
    printf("not enough memory (realloc returned NULL)\n");
    return 0;
  }
=20
  mem-&gt;memory =3D ptr;
  memcpy(&amp;(mem-&gt;memory[mem-&gt;size]), contents, realsize);
  mem-&gt;size +=3D realsize;
  mem-&gt;memory[mem-&gt;size] =3D 0;
=20
  return realsize;
}
=20
